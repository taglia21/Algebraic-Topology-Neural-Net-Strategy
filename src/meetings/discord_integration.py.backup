    id='weekly_review',
    name='Weekly Deep Dive'
)

# ML retraining check - Daily at 6 PM EST
scheduler.add_job(
    ml_retraining_check,
    CronTrigger(day_of_week='mon-fri', hour=18, minute=0),
    id='ml_retraining',
    name='ML Retraining Check'
)

if __name__ == "__main__":
